<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="50.0" prefWidth="940.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adminpanelpage.PostDataRowsController">
   <children>
      <Button fx:id="viewPostButton" layoutX="690.0" layoutY="11.0" mnemonicParsing="false" onAction="#viewPost" style="-fx-background-color: #00aaff;" text="View Post" textFill="WHITE">
         <font>
            <Font name="Georgia" size="15.0" />
         </font>
      </Button>
      <Button fx:id="removePostButton" layoutX="802.0" layoutY="11.0" mnemonicParsing="false" onAction="#removePost" style="-fx-background-color: red;" text="Remove Post" textFill="WHITE">
         <font>
            <Font name="Georgia" size="15.0" />
         </font>
      </Button>
      <Label fx:id="id" alignment="CENTER" layoutY="1.0" prefHeight="50.0" prefWidth="44.0" text="20" textFill="#00aaff" wrapText="true">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font>
      </Label>
      <Label fx:id="title" alignment="CENTER" layoutX="44.0" layoutY="1.0" prefHeight="50.0" prefWidth="170.0" text="Urgent Blood" textFill="#00aaff" wrapText="true">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font>
      </Label>
      <Label fx:id="bloodGroup" alignment="CENTER" layoutX="214.0" layoutY="1.0" prefHeight="50.0" prefWidth="68.0" text="O+" textFill="#00aaff" wrapText="true">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font>
      </Label>
      <Label fx:id="location" alignment="CENTER" layoutX="282.0" layoutY="1.0" prefHeight="50.0" prefWidth="294.0" text="Dhaka,  Dhaka , Dhaka" textFill="#00aaff" wrapText="true">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font>
      </Label>
      <Label fx:id="status" alignment="CENTER" layoutX="576.0" layoutY="1.0" prefHeight="50.0" prefWidth="107.0" text="No Donor Found" textFill="RED" wrapText="true">
         <font>
            <Font name="Arial Bold" size="11.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
